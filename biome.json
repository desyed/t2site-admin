{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"style": {
				"useBlockStatements": "error"
			},
			"suspicious": {
				"noConsoleLog": "error",
				"noArrayIndexKey": "off"
			},
			"correctness": {
				"noUnusedVariables": "error"
			}
		}
	},
	"formatter": {}
}
