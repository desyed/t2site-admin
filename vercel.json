{
	"trailingSlash": true,
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/"
		},
		{
      "source": "/server/:match*",
      "destination": "https://uqify.com/:match*"
    }
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "x-powered-by",
					"value": "t2site"
				}
			]
		}
	]
}
